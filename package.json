{
  "name": "rest-client-sdk",
  "version": "v4.0.0",
  "description": "Rest Client SDK for API",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "build": "rollup -c",
    "check:src": "npm run test",
    "prepublishOnly": "npm run clean && npm run check:src && npm run build",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "deep-diff": "^1.0.2",
    "urijs": "^1.19.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "bundlesize": "^0.17.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "fetch-mock": "^5.13.1",
    "form-data": "^2.3.3",
    "husky": "^1.1.4",
    "immutable": "^4.0.0-rc.12",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^1.7.4",
    "lint-staged": "^8.0.4",
    "pluralize": "^7.0.0",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "optionalDependencies": {
    "pluralize": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapado/rest-client-js-sdk.git"
  },
  "keywords": [
    "Rest",
    "SDK",
    "API"
  ],
  "author": "Mapado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapado/rest-client-js-sdk/issues"
  },
  "homepage": "https://github.com/mapado/rest-client-js-sdk#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bundlesize": [
    {
      "path": "./dist/index.es.js",
      "maxSize": "11 Kb"
    }
  ],
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "yarn run prettier --write",
      "git add"
    ]
  },
  "jest": {
    "automock": false,
    "setupTestFrameworkScriptFile": "./setupJest.js"
  }
}
