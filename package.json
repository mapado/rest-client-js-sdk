{
  "name": "rest-client-sdk",
  "version": "4.1.2",
  "description": "Rest Client SDK for API",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "build": "rollup -c",
    "check:src": "npm run test",
    "prepublishOnly": "npm run clean && npm run check:src && npm run build"
  },
  "dependencies": {
    "deep-diff": "^1.0.2",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "bundlesize": "^0.18.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0 || ^1.7.0",
    "fetch-mock": "^5.13.1",
    "form-data": "^2.3.3",
    "husky": "^4.2.3",
    "immutable": "^4.0.0-rc.12",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^1.7.4",
    "lint-staged": "^10.1.2",
    "pluralize": "^8.0.0",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.0",
    "rollup": "^2.3.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "optionalDependencies": {
    "pluralize": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapado/rest-client-js-sdk.git"
  },
  "keywords": [
    "Rest",
    "SDK",
    "API"
  ],
  "author": "Mapado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapado/rest-client-js-sdk/issues"
  },
  "homepage": "https://github.com/mapado/rest-client-js-sdk#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bundlesize": [
    {
      "path": "./dist/index.es.js",
      "maxSize": "11 Kb"
    }
  ],
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "yarn run prettier --write"
    ]
  },
  "jest": {
    "automock": false,
    "setupTestFrameworkScriptFile": "./setupJest.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
