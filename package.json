{
  "name": "rest-client-sdk",
  "version": "v2.2.1",
  "description": "Rest Client SDK for API",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": ["dist", "src"],
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "build": "rollup -c",
    "check:src": "npm run test",
    "prepublishOnly": "npm run clean && npm run check:src && npm run build",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "deep-diff": "^0.3.8",
    "urijs": "^1.19.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^23.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "bundlesize": "^0.17.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.4.0",
    "fetch-mock": "^5.13.1",
    "form-data": "^2.1.2",
    "husky": "^1.0.0",
    "immutable": "^4.0.0-rc.9",
    "jest": "^23.1.0",
    "lint-staged": "^7.0.3",
    "pluralize": "^7.0.0",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "optionalDependencies": {
    "pluralize": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapado/rest-client-js-sdk.git"
  },
  "keywords": ["Rest", "SDK", "API"],
  "author": "Mapado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapado/rest-client-js-sdk/issues"
  },
  "homepage": "https://github.com/mapado/rest-client-js-sdk#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bundlesize": [
    {
      "path": "./dist/index.es.js",
      "maxSize": "11 Kb"
    }
  ],
  "lint-staged": {
    "*.{js,jsx,json,md}": ["yarn run prettier --write", "git add"]
  },
  "jest": {
    "setupFiles": ["./setupJest.js"]
  }
}
