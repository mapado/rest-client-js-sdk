{
  "name": "rest-client-sdk",
  "version": "1.2.0",
  "description": "Rest Client SDK for API",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": ["dist", "src"],
  "scripts": {
    "clean": "rimraf dist",
    "test":
      "mocha --compilers js:babel-core/register --require ./test/test_helper.js  --recursive",
    "test:watch": "npm run test -- --watch",
    "build": "rollup -c",
    "check:src": "npm run test",
    "prepublishOnly": "npm run clean && npm run check:src && npm run build",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "urijs": "^1.18.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "bundlesize": "^0.14.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "fetch-mock": "^4.6.0",
    "form-data": "^2.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "mocha": "^3.2.0",
    "prettier": "^1.6.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapado/rest-client-js-sdk.git"
  },
  "keywords": ["Rest", "SDK", "API"],
  "author": "Mapado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapado/rest-client-js-sdk/issues"
  },
  "homepage": "https://github.com/mapado/rest-client-js-sdk#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bundlesize": [
    {
      "path": "./dist/index.es.js",
      "threshold": "31 Kb"
    }
  ],
  "lint-staged": {
    "*.{js,jsx,json}": ["prettier --write", "git add"]
  }
}
